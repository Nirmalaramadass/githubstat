import matplotlib.pyplot as plt

# Sample contribution data (replace with yours)
days = list(range(1, 32))
contributions = [
    1,0,0,0,0,1,4,0,2,0,0,0,0,0,
    1,3,0,0,0,10,3,0,6,0,9,2,1,7,1,0
]

plt.style.use("dark_background")

plt.figure(figsize=(14, 5))
plt.plot(days, contributions, color="#ff4d6d", marker="o", linewidth=2)

plt.title("Naveenraj Thiyagarajan's Contribution Graph", color="#ff4d6d")
plt.xlabel("Days")
plt.ylabel("Contributions")

plt.grid(True, linestyle="--", alpha=0.3)
plt.tight_layout()
plt.show()
